    "pm2": "^4.5.6",
    "reflect-metadata": "^0.1.13",
    "request": "^2.88.0",
    "request-context": "^2.0.0",
    "request-promise": "^4.2.4",
    "swagger-ui-express": "^4.0.2",
    "typeorm": "^0.2.32",
    "typescript": "^4.2.3",
    "uuid": "^8.3.2",
    "winston": "^3.2.1",
    "winston-daily-rotate-file": "^3.9.0",
    "xml2js": "^0.4.23"
  },
