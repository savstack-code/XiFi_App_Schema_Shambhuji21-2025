{
  "name": "xifi",
  "version": "3.0.0",
  "description": "xifi hotspot",
  "main": "src/service.js",
  "scripts": {
    "dev": "tsc-watch --onSuccess \"node ./dist/service.js\"",
    "postinstall": "npx tsc",
    "copyfiles": "copyfiles public/**/*.html public/**/*.css public/**/*.png public/**/*.jpg public/**/*.mp4 dist/",
    "start": "npx pm2 start pm2.yaml",
    "stop": "npx pm2 stop pm2.yaml",
    "test": "echo \"Error: no test specified\" && exit 1",
    "typeorm:cli": "ts-node ./node_modules/typeorm/cli.js -f ./src/typeormconfig.ts",
    "build": "npx tsc && cd dist && zip -r ../distApp.zip * && cd .. && echo \"Package built: distApp.zip\"",
    "local-script": "ts-node ./src/scripts/index.ts"
  },
  "author": "",
  "license": "ISC",
